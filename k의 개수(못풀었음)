class Solution {
    public int solution(int i, int j, int k) {
        int answer = 0;
        
        for(int n = i; n<=j; n++) {
            if(Integer.toString(n).contains(Integer.toString(k))) {
                answer++;
                if(Integer.toString(n).matches("(.*)"+Integer.toString(k)+"(.*)"+Integer.toString(k)+"(.*)")) {
                    answer++;
                }
            }
        }
        
        return answer;
    }
}

// 왜 틀렸는지 모르겠음.
